<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="30dip"
    tools:ignore="HardcodedText">

    <LinearLayout
        android:id="@+id/rl"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:orientation="vertical"
        android:padding="10dip"
        tools:ignore="UselessParent">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dip"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical|left"
                android:text="@string/layout_my_address_dialog0"
                android:textColor="#000000"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llSheng"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/layout_my_address_dialog1"
                android:textColor="#6D6D6D"
                android:textSize="15dp" />

            <Spinner
                android:id="@+id/spinner_shengID"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dip"
                android:animationCache="true"
                android:background="@drawable/spinner_selector"
                android:drawSelectorOnTop="true"
                android:padding="0dip"
                android:textColor="#6d6d6d"
                android:textSize="15dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llShi"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/layout_my_address_dialog2"
                android:textColor="#6D6D6D"
                android:textSize="15dp" />

            <Spinner
                android:id="@+id/spinner_shiID"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dip"
                android:animationCache="true"
                android:background="@drawable/spinner_selector"
                android:drawSelectorOnTop="true"
                android:padding="0dip"
                android:textColor="#6d6d6d"
                android:textSize="15dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llQu"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/layout_my_address_dialog3"
                android:textColor="#6D6D6D"
                android:textSize="15dp" />

            <Spinner
                android:id="@+id/spinner_quID"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dip"
                android:animationCache="true"
                android:background="@drawable/spinner_selector"
                android:drawSelectorOnTop="true"
                android:padding="0dip"
                android:textColor="#6d6d6d"
                android:textSize="15dp" />
        </LinearLayout>

        <!--<LinearLayout-->
            <!--android:id="@+id/llJie"-->
            <!--android:layout_width="fill_parent"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginTop="5dip"-->
            <!--android:gravity="center_vertical"-->
            <!--android:orientation="horizontal">-->

            <!--<TextView-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:text="@string/layout_my_address_dialog4"-->
                <!--android:textColor="#6D6D6D"-->
                <!--android:textSize="15dp" />-->

            <!--<Spinner-->
                <!--android:id="@+id/spinner_jieID"-->
                <!--android:layout_width="fill_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_marginLeft="5dip"-->
                <!--android:animationCache="true"-->
                <!--android:background="@drawable/spinner_selector"-->
                <!--android:drawSelectorOnTop="true"-->
                <!--android:padding="0dip"-->
                <!--android:textColor="#6d6d6d"-->
                <!--android:textSize="15dp" />-->
        <!--</LinearLayout>-->
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btu_on"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="#D9434E"
            android:padding="10dip"
            android:text="@string/layout_my_address_dialog5"
            android:textColor="#FFFFFF"
            android:textSize="18dp" />

        <View
            android:layout_width="0.5dip"
            android:layout_height="fill_parent"
            android:background="#DCDEE0" />

        <Button
            android:id="@+id/btu_off"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#909090"
            android:padding="10dip"
            android:text="@string/layout_my_address_dialog6"
            android:textColor="#FFFFFF"
            android:textSize="18dp" />
    </LinearLayout>

</RelativeLayout>